{
  "name": "mockoon",
  "description": "Mockoon is the easiest and quickest way to run mock APIs locally. No remote deployment, no account required, open source.",
  "version": "1.5.1",
  "author": {
    "name": "Guillaume Monnet",
    "email": "hi@255kb.com",
    "url": "https://255kb.com/"
  },
  "license": "MIT",
  "repository": "https://github.com/mockoon/mockoon",
  "homepage": "https://mockoon.com",
  "bugs": {
    "url": "https://github.com/mockoon/mockoon/issues"
  },
  "browserslist": [
    "Electron >= 6.0"
  ],
  "scripts": {
    "lint": "ng lint --type-check",
    "test": "ts-mocha -p test/tsconfig.spec.json --timeout 10000 test/**/*.spec.ts",
    "setversion": "node ./scripts/version-bump.js",
    "serve": "concurrently \"npm run serve:app\" \"wait-on -d 10000 ./dist && npm run serve:electron\"",
    "serve:app": "ng build --configuration=development --watch --output-hashing=all",
    "serve:electron": "electron ./dist --serve --remote-debugging-port=8888",
    "build:dev": "rm -rf ./dist && ng build --prod --configuration=development",
    "build:prod": "rm -rf ./dist && ng build --prod --configuration=production",
    "clean-packages": "rm -rf ./packages",
    "package:win": "npm run clean-packages && electron-builder --win --config electron-builder.json",
    "package:linux": "npm run clean-packages && electron-builder --linux --config electron-builder.json",
    "package:mac": "npm run clean-packages && electron-builder --mac --config electron-builder.json"
  },
  "dependencies": {
    "@angular/animations": "8.2.6",
    "@angular/common": "8.2.6",
    "@angular/compiler": "8.2.6",
    "@angular/core": "8.2.6",
    "@angular/forms": "8.2.6",
    "@angular/platform-browser": "8.2.6",
    "@angular/platform-browser-dynamic": "8.2.6",
    "@angular/router": "8.2.6",
    "@ng-bootstrap/ng-bootstrap": "5.1.1",
    "axios": "^0.19.0",
    "bootstrap": "4.3.1",
    "brace": "0.11.1",
    "dummy-json": "2.0.0",
    "electron-is-dev": "1.1.0",
    "electron-json-storage": "4.1.0",
    "electron-window-state": "5.0.3",
    "express": "4.17.1",
    "firebase": "6.6.1",
    "http-proxy-middleware": "0.20.0",
    "killable": "1.0.1",
    "lodash": "4.17.15",
    "material-design-icons": "3.0.1",
    "mime-types": "2.1.24",
    "ng2-ace-editor": "0.3.9",
    "ng2-dragula": "2.1.1",
    "ngx-markdown": "8.1.0",
    "object-path": "0.11.4",
    "request": "2.88.0",
    "rxjs": "6.5.3",
    "semver": "6.3.0",
    "spinthatshit": "1.0.4",
    "timers": "0.1.1",
    "uuid": "3.3.3",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "8.2.0",
    "@angular-builders/dev-server": "7.3.1",
    "@angular-devkit/build-angular": "0.803.4",
    "@angular-devkit/core": "8.3.4",
    "@angular/cli": "8.3.4",
    "@angular/compiler-cli": "8.2.6",
    "@angular/fire": "5.2.1",
    "@angular/language-service": "8.2.6",
    "@types/chai": "4.2.2",
    "@types/chai-as-promised": "7.1.2",
    "@types/express": "4.17.1",
    "@types/mocha": "5.2.7",
    "@types/node": "12.7.5",
    "@types/webdriverio": "4.13.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "codelyzer": "5.1.0",
    "concurrently": "4.1.2",
    "date-fns": "1.30.1",
    "dotenv": "^8.1.0",
    "electron": "4.2.10",
    "electron-builder": "21.2.0",
    "electron-notarize": "^0.1.1",
    "electron-reload": "1.4.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "mkdirp": "0.5.1",
    "mocha": "6.1.4",
